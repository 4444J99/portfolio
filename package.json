{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "generate-data": "python3 ../ingesting-organ-document-structure/scripts/praxis-portfolio-generate.py --output-dir src/data && cp ../ingesting-organ-document-structure/system-metrics.json src/data/",
    "build": "astro build && npx pagefind --site dist --glob '**/*.html'",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "node scripts/a11y-audit.mjs",
    "lighthouse": "lhci autorun --config=lighthouserc.cjs"
  },
  "dependencies": {
    "@astrojs/rss": "4.0.15",
    "@astrojs/sitemap": "3.7.0",
    "astro": "^5.17.1",
    "d3": "7.9.0",
    "mermaid": "11.12.2",
    "p5": "2.2.1"
  },
  "devDependencies": {
    "@lhci/cli": "0.15.1",
    "@resvg/resvg-js": "2.6.2",
    "@types/d3": "7.4.3",
    "@types/p5": "1.7.7",
    "@vitest/coverage-v8": "4.0.18",
    "axe-core": "4.11.1",
    "cheerio": "1.2.0",
    "jsdom": "28.1.0",
    "pagefind": "1.4.0",
    "satori": "0.19.2",
    "vitest": "4.0.18"
  },
  "overrides": {
    "lodash-es": "4.17.23"
  }
}
